{
    "workbench.colorTheme": "Default High Contrast",
    "workbench.startupEditor": "none",
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.tabCompletion": "on",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnType": true,
    "[markdown]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "diffEditor.ignoreTrimWhitespace": true
    },
    "markdown-preview-github-styles.colorTheme": "dark",
    "solidity.telemetry": false,
    "markdown-preview-github-styles.darkTheme": "dark_colorblind",
    "editor.guides.bracketPairs": true,
    "workbench.iconTheme": "material-icon-theme",
    "indentRainbow.colors": [

        "rgba(255,255,64,0.9)",
        "rgba(127,255,127,0.9)",
        "rgba(255,127,255,0.9)",
        "rgba(217,57,0,0.9)"
    ],
    "indentRainbow.errorColor": "rgba(128,32,32,0.6)",
    "indentRainbow.tabmixColor": "rgba(128,32,96,0.6)",
    "indentRainbow.updateDelay": 10,
    "indentRainbow.indicatorStyle": "light",
    "indentRainbow.lightIndicatorStyleLineWidth": 1,
    "indentRainbow.excludedLanguages": [
        "solidity",
        "c",
        "json",
        "rust",
        "assembly",
        "asm",
        "bytecode",
        "byte"
    ],
    "cSpell.userWords": [
        "bytedance",
        "vuln"
    ],
    "markdown-preview-enhanced.alwaysShowBacklinksInPreview": true,
    "markdown-preview-enhanced.frontMatterRenderingOption": "code block",
    "markdown-preview-enhanced.previewTheme": "github-dark.css",
    "editor.formatOnPaste": true,
    "workbench.editor.tabSizing": "shrink",
    "prettier.tabWidth": 4,
    "prettier.useTabs": true,
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "explorer.confirmDelete": false,
    "python.createEnvironment.trigger": "off",
    "terminal.integrated.persistentSessionScrollback": 10000,
    "terminal.integrated.scrollback": 10000,
    "git.openRepositoryInParentFolders": "never",
    "extensions.ignoreRecommendations": true,
    "workbench.editor.enablePreview": false
}
